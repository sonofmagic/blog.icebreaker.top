{
  "name": "monorepo-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "vitest run --coverage.enabled",
    "test:dev": "vitest",
    "lint": "turbo run lint",
    "release": "changeset",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "commit",
    "script:init": "monorepo init",
    "script:sync": "monorepo sync",
    "script:clean": "monorepo clean",
    "script:mirror": "monorepo mirror"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@icebreakers/eslint-config": "^1.1.8",
    "@icebreakers/monorepo": "^1.0.3",
    "@icebreakers/stylelint-config": "^1.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/get-value": "^3.0.5",
    "@types/klaw": "^3.0.7",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash": "^4.17.17",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.15.30",
    "@types/set-value": "^4.0.3",
    "@vitest/coverage-v8": "~3.2.2",
    "ci-info": "^4.2.0",
    "cross-env": "^7.0.3",
    "defu": "^6.1.4",
    "eslint": "^9.28.0",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "get-value": "^4.0.1",
    "husky": "^9.1.7",
    "klaw": "^4.1.0",
    "lint-staged": "^16.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.8",
    "only-allow": "^1.2.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "rimraf": "^6.0.1",
    "set-value": "^4.1.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "~3.2.2",
    "yaml": "^2.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "pnpm": {
    "overrides": {
      "vue": "^2.7",
      "vue-server-renderer": "^2.7",
      "vue-template-compiler": "^2.7"
    }
  },
  "resolutions": {
    "vue": "^2.7",
    "vue-server-renderer": "^2.7",
    "vue-template-compiler": "^2.7"
  }
}
