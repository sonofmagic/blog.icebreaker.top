  "license": "MIT",
  "scripts": {
    "dev": "npm run rollup:listener && npm run start:listener",
    "dev:raw": "cross-env DEBUG=nuxt:* nuxt dev",
    "dev:brk": "cross-env NODE_ENV=development SLS_ENV=dev DEBUG=nuxt:* node --inspect-brk=9229 server/listener.js",
    "dev:sls": "serverless dev",
    "sync": "node ./scripts/syncArticles.js",
    "analyze": "cross-env SLS_ENV=release nuxt build --analyze",
    "generate": "cross-env target=static nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "rollup:listener": "cross-env NODE_ENV=development rollup -c",
    "start:listener": "cross-env NODE_ENV=development SLS_ENV=dev DEBUG=nuxt:* node --inspect=9229 dist/listener.js",
    "gen": "cross-env SLS_ENV=release NODE_ENV=production node ./scripts/genCfg.js",
    "build:local": "cross-env SLS_ENV=test nuxt build",
    "build:sls": "cross-env SLS_ENV=release nuxt build",
    "start:sls": "cross-env SLS_ENV=release nuxt start",
    "start": "cross-env SLS_ENV=release NODE_ENV=production node --inspect=9229 dist/listener.js",
    "build:server": "cross-env NODE_ENV=production rollup -c",
    "deploy": "npm run gen && npm run build:server && npm run build:sls && npm run deploy:cdn && serverless deploy --no-color",
    "deploy:layer": "node ./scripts/deployLayer.js",
    "deploy:cdn": "cross-env SLS_ENV=release node ./scripts/cdn.js",
    "deploy:static": "cross-env SLS_ENV=release node ./scripts/cloudbase.js",
    "check:upgrade": "yarn upgradeInteractive --latest"
  },
  "commentDependencies": {
    "for-yarn-upgrade": true,
